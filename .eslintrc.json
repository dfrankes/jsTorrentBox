{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
